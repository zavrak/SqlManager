@{
    ViewData["Title"] = "Home Page";
}
@model List<Dataplatform.SQLManager.Web.DataModels.Script>

    <div class="text-center">
        <img src="~/DP.png" width="100" height="100"  />

    </div>
<div class="text-center">
    <h1 class="display-4">Data Platform</h1>


    @using (Html.BeginForm("Index", "Home", FormMethod.Post))
    {
        <input type="text" name="keyword" id="keyword" class="form-control" />
        <br />
        <button type="submit" class="btn btn-success">ARA</button>
    }
</div>
<br />
<br />
<br />

<div class="row">
    <div class="col">
        <h3>Search Results</h3>
        <ul>
            @foreach (var item in Model)
            {
            <li>
                <b>@item.Title</b>
                @Html.ActionLink("Edit","Edit","Home",new {Id = item.Id }) |
                @Html.ActionLink("Details", "Details", "Home", new { Id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", "Home",  new { Id = item.Id })
            </li>
      
            }
        </ul>
    </div>
    <div class="col">
      


    </div>
</div>



